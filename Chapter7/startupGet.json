[
  {
    "description": "模拟一个没有参数的get请求",
    "request": {
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "text": "这是一个没有参数的get请求",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  },
  {
    "description": "这是一个带参数的get",
    "request": {
      "uri": "/getwithparam",
      "method": "get",
      "queries": {
        "name": "魏无羡",
        "age": "18"
      }
    },
    "response": {
      "text": "蓝忘机我来了",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }

  }
]