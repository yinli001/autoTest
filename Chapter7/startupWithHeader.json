[
  {
    "description": "这是一个带header信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "queries": {
        "name": "sizhui",
        "sex": "woman"
      }
    },
    "response": {
      "json": {
        "sizhui": "success",
        "status":"1"
      }
    }
  }
]